{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},

	"AllowedHosts": "*",

	"ReverseProxy": {
		"Routes": {
			"BookingBase": {
				"ClusterId": "Booking",
				"Match": {
					"Path": "{**catch-all}"
				},
				"Transforms": []
			}
		},
		"Clusters": {
			"Booking": {
				"LoadBalancingPolicy": "RoundRobin",
				"Destinations": {
					"Base_Server1": {
						"Address": "https://localhost:7229/"
					},
					"Base_Server2": {
						"Address": "https://localhost:7229/"
					}
				}
			}
		}
	},

	"LockWaitTimeMs": 10000,
	"LockExpiryMs": 30000,
	"LockRetryMs": 1000,
	"RedisEndpoints": [ "localhost" ]
}